nt\ntool_agent = create_agent(\n    model=llm,\n    tools=[get_weather, math_add],\n    system_prompt=\"You are a helpful assistant\",\n)\n\n# 在运行Agent时注入context\nresponse = tool_agent.invoke(\n    {\"messages\": [{\"role\": \"user\", \"content\": \"请计算 8234783 + 94123832 = ?\"}]},\n    config={\"configurable\": {\"thread_id\": \"1\"}},\n    context=Context(authority=\"admin\"),\n)","key":"H2xfLmXwSy"},{"type":"output","id":"jtQTvb2gORkZqO25vhvP3","data":[],"key":"xUyLcmJiEl"}],"key":"GpVL1vhCes"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"for message in response['messages']:\n    message.pretty_print()","key":"eeEObwVBjz"},{"type":"output","id":"SOtbOhl-8tEiWHFj0DwA9","data":[{"name":"stdout","output_type":"stream","text":"================================\u001b[1m Human Message \u001b[0m=================================\n\n请计算 8234783 + 94123832 = ?\n==================================\u001b[1m Ai Message \u001b[0m==================================\nTool Calls:\n  math_add (call_985e4f898e064650885c30c7)\n Call ID: call_985e4f898e064650885c30c7\n  Args:\n    a: 8234783\n    b: 94123832\n=================================\u001b[1m Tool Message \u001b[0m=================================\nName: math_add\n\n102358615\n==================================\u001b[1m Ai Message \u001b[0m==================================\n\n8234783 + 94123832 = 102358615。\n"}],"key":"dHgWoeAVjB"}],"key":"yiKDahKmRK"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"# 验证计算结果是否正确\n8234783 + 94123832","key":"THgsl7sY0W"},{"type":"output","id":"gTzLqAbzNZBECRuHr1Yxx","data":[{"output_type":"execute_result","execution_count":10,"metadata":{},"data":{"text/plain":{"content":"102358615","content_type":"text/plain"}}}],"key":"lOEV2uG6mY"}],"key":"v6MyVSYq0l"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column"